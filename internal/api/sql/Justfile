	# $vlNOanyone_callback_inactive_listsSQL = "and vicidial_list.list_id NOT IN($inactive_lists)";
	sed -i "s|\$vlNOanyone_callback_inactive_listsSQL|and vicidial_list.list_id NOT IN(\$inactive_lists)|g" 		{{file}}
