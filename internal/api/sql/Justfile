astguiRoot := "$HOME/ghq/git.p1.rocks/gitops/goautodial.k8s/app/astguiclient/"
schemaTarget := "./generated"
sourcePath := "./src-copy"

default:
	rm -r {{schemaTarget}} {{sourcePath}}
	mkdir -p {{sourcePath}} {{schemaTarget}}
	just extract-schema
	just extract-queries
	just generate-table-sql
	just categorize-files

extract-schema:
	#! /usr/bin/env bash
	# copy over schema
	cp {{astguiRoot}}/extras/MySQL_AST_CREATE_tables.sql \
	   {{sourcePath}}/schema.sql
	# Some fixes required for sqlc
	sed -i "s|DOUBLE|FLOAT|g" 								{{sourcePath}}/schema.sql
	sed -i "s|^action |\`action\` |g" 						{{sourcePath}}/schema.sql
	sed -i "s|^status |\`status\` |g" 						{{sourcePath}}/schema.sql
	sed -i "s|^200_date |\`200_date\` |g" 					{{sourcePath}}/schema.sql
	sed -i "s|PRIMARY KEY NOT NULL|PRIMARY KEY|g" 			{{sourcePath}}/schema.sql
	sed -i "s|UNIQUE NOT NULL|NOT NULL UNIQUE|g" 			{{sourcePath}}/schema.sql
	sed -i "s|NOT NULL UNIQUE PRIMARY KEY|PRIMARY KEY|g" 	{{sourcePath}}/schema.sql
	# uniform some keywords for some regexes in this file
	sed -i "s| limit | LIMIT |g" 							{{sourcePath}}/schema.sql
	sed -i "s|count(|COUNT(|g" 								{{sourcePath}}/schema.sql
	sed -i "s|select |SELECT |g" 							{{sourcePath}}/schema.sql
	sed -i "s|insert |INSERT |g" 							{{sourcePath}}/schema.sql
	sed -i "s|update |UPDATE |g" 							{{sourcePath}}/schema.sql
	sed -i "s|delete |DELETE |g" 							{{sourcePath}}/schema.sql


generate-table-sql:
	#! /usr/bin/env bash
	# extract create alter table statments only
	while read -rd ';' sql; do
	  pat='^(CREATE|ALTER) TABLE (\w+).*'
	  if [[ "$sql" =~ $pat ]]; then
	    echo "$sql;" >> 										"{{schemaTarget}}/${BASH_REMATCH[2]}.sql"
	  fi
	done < 														{{sourcePath}}/schema.sql
	# remove unused tables
	for file in $(fd .sql {{schemaTarget}}); do
	  table=$(basename "${file%.sql}")
	  [[ ! $(rg $table {{sourcePath}}/all-crud-statements.sql) ]] && rm $file
	done
	# append query statments per table
	table_array=($(fd .sql {{schemaTarget}} | xargs -n1 -I{} basename {} | cut -f 1 -d '.'))
	while read -r line; do
	  for table in ${table_array[@]}; do
	     pattern="( |,)$table(,|;| )"
	     if [[ "$line" =~ $pattern ]]; then
	        pat='(bin/.*\.pl:[0-9]+):(.*)'; [[ "$line" =~ $pat ]]
	        file="${BASH_REMATCH[1]}"
	        stmt="${BASH_REMATCH[2]}"
	        echo -e "\n\n" >> 									"{{schemaTarget}}/$table.sql"
	        echo "/* $file */" >> 								"{{schemaTarget}}/$table.sql"
	        echo -e "\n" >> 									"{{schemaTarget}}/$table.sql"
	        camelCaseTable=$(echo "$table" | sed -r 's/(^|_)([a-z])/\U\2/g')
	        sel_count_pat='^SELECT COUNT'
	        if [[ "$stmt" =~ $sel_count_pat ]]; then
	          prefix="Count"
	    	  op="one"
	        elif [[ "$stmt" =~ ^SELECT ]]; then
	          prefix="Get"
	          pat_select='LIMIT 1;'
	          if [[ "$stmt" =~ $pat_select ]]; then
	    	    op="one"
	          else
	            op="many"
	          fi
	        elif [[ "$stmt" =~ ^DELETE ]]; then
	          prefix="Delete"
	          op="exec"
	        elif [[ "$stmt" =~ ^INSERT ]]; then
	          prefix="Create"
	          op="execresult"
	        elif [[ "$stmt" =~ ^UPDATE ]]; then
	          prefix="Update"
	          op="exec"
	        fi
	        echo "/* name: $prefix$camelCaseTable :$op */" >> 	"{{schemaTarget}}/$table.sql"
	        echo "$stmt" >> 									"{{schemaTarget}}/$table.sql"
	     fi
	  done
	done < 														{{sourcePath}}/all-crud-statements.sql

clear-categorize-files:
	rm -rf {{schemaTarget}}/channels || true
	rm -rf {{schemaTarget}}/metadata || true
	rm -rf {{schemaTarget}}/vicilogs || true
	rm -rf {{schemaTarget}}/manager  || true
	rm -rf {{schemaTarget}}/server   || true

