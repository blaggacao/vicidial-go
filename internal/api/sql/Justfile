query:
	rg -nH -v '^\s*#' {{astguiRoot}}/bin | sed 's|\\n||g' | sed 's|\\t||g' | sed 's|\\"|'"'"'|g' | sed 's|\\||g' | \
	  rg -oS \
	    '(^.*?:\d+:).*?"((?:update|delete|select|insert|create|alter|lock tables|unlock tables)[^;]+);' \
	    -r '$1$2' | sed 's|"$||g'

extract-queries:
	#! /usr/bin/env bash
	rg -nH -v '^\s*#' {{astguiRoot}}/bin \
	  | sed 's|\\n||g' \
	  | sed 's|\\t||g' \
	  | sed 's|\\"|'"'"'|g' \
	  | sed 's|\\||g' | \
	  rg -oS \
	    '(^.*?:\d+:).*?"((?:update|delete|select|insert|create|alter|lock tables|unlock tables)[^;]+);' \
	    -r '$1$2;' \
	    | sed 's|";$|;|g' \
	    | sed "s|\"|'|g" \
	    | sed "s| ' . | |g" \
	    > 									{{sourcePath}}/all-statements.sql
	just _cleanup-sql 						{{sourcePath}}/all-statements.sql
	just _clean-queries 					{{sourcePath}}/all-statements.sql
	cp 										{{sourcePath}}/all-statements.sql \
	   										{{sourcePath}}/all-statments-unpurged.sql
	just _purge-unused 						{{sourcePath}}/all-statements.sql
	while read -r line; do
	  pat='(bin/.*\.pl:[0-9]+):(.*)'; [[ "$line" =~ $pat ]]
	  stmt="${BASH_REMATCH[2]}"
	  if [[ "$stmt" =~ (^SELECT|^UPDATE|^INSERT|^DELETE) ]]; then
	    echo "$line" >> 					{{sourcePath}}/all-crud-statements.sql
	  else
	    echo "$line" >> 					{{sourcePath}}/all-non-crud-statements.sql
	  fi
	done < 									{{sourcePath}}/all-statements.sql

regex:
	regex="'\\\$.*?(?<!{)'(?!})"; echo $regex

_clean-queries file="./all-statements.sql":
	sed -i "s@{{astguiRoot}}/@@g" 							{{file}} # Remove unnecessary path prefix
	sed -i "s|UNIX_TIMESTAMP|UTC_TIMESTAMP|g" 				{{file}} # Though valid spec, sqlc doesn't recognize UNIX_TIMESTAMP
	regex="'\\\$(.*?(?<!{))'(?!})"; \
	  perl -pi -e "s|$regex|?/* \1 */|g" 					{{file}} # replace '$---' & '$---{'--'}' variables by '?'
	regex="'' \. \\\$(.*?) \. ''"; \
	  perl -pi -e "s|$regex|?/* \1 */|g" 					{{file}} # replace  '' . $--- . '' variables by '?'
	regex="'' \. \\\$(.*?) \.''"; \
	  perl -pi -e "s|$regex|?/* \1 */|g" 					{{file}} # replace  '' . $--- .'' variables by '?'
	regex="\\\$(.+?})"; \
	  perl -pi -e "s|$regex|?/* \1 */|g" 					{{file}} # replace $---{'--'} variables by '?'
	regex="=\\\$(\w+)"; \
	  perl -pi -e "s|$regex|=?/* \1 */|g" 					{{file}} # replace  =$--- variables by '?'
	regex="'([%A-Za-z: ]+?\\\$.*?)'"; \
	  perl -pi -e "s|$regex|?/* \"\1\" */|g" 		 		{{file}} # replace '--- $----' variables by '?'
	regex="\(' \. \("; \
	  perl -pi -e "s|$regex||g" 		 					{{file}} # replace (' . ( by
	regex="\(' \. "; \
	  perl -pi -e "s|$regex||g" 		 					{{file}} # replace (' .  by
	regex="\) \. '\)"; \
	  perl -pi -e "s|$regex||g" 		 					{{file}} # replace ) . ') by
	regex=" \. ' ?\)"; \
	  perl -pi -e "s|$regex||g" 		 					{{file}} # replace  . ') or  . ' ) by

